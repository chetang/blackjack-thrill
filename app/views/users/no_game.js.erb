$(".stats-result").html("")
var html = ""
<% if @inactive_users.length > 0 %>
	<% @inactive_users.each_with_index do |v, index| %>
	  html += "<div><%= index +1 %> : <%= v.email %></div>"
	<% end %>
<% else %>
	html = "<div class='row text-center'>Everyone is active and playing games!!!</div>"
<% end %>
$(".stats-result").html(html)
